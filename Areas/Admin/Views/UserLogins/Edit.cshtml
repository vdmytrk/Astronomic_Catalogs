@model Astronomic_Catalogs.Areas.Admin.Models.AspNetUserLogin

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Admin/Views/_ViewStart.cshtml";
}

<h1>Edit</h1>

<h4>AspNetUserLogin</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="LoginProvider" />
            <input type="hidden" asp-for="ProviderKey" />
            <div class="form-group">
                <label asp-for="ProviderDisplayName" class="control-label"></label>
                <input asp-for="ProviderDisplayName" class="form-control" />
                <span asp-validation-for="ProviderDisplayName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UserId" class="control-label"></label>
                <select asp-for="UserId" class="form-control" asp-items="ViewBag.UserId"></select>
                <span asp-validation-for="UserId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
